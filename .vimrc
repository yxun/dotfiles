:set number
:set ts=4
" :set list

:set history=20

syntax on
colorscheme predawn

set autoindent
set expandtab
set shiftwidth=4
set showmatch
let python_hightlight_all=1

" vim-go Highlight
let g:go_highlight_functions = 1
let g:go_highlight_methods = 1
let g:go_highlight_structs = 1
let g:go_highlight_operators = 1
let g:go_highlight_build_constraints = 1
